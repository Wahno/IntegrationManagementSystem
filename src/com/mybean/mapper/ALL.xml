<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">	
    <mapper namespace="com.mybean.mapper.AllMapper">
    	
        <insert id="add" parameterType="All" >
            insert into Buy_Type (Uid,Acredits) values (#{Uid},#{Acredits})   
        </insert>
         
        <delete id="delete" parameterType="_int" >
            delete from Buy_Type where Uid= #{Uid}  
        </delete>
         
        <select id="get" parameterType="_int" resultType="All">
            select * from Buy_Type  where Uid= #{Uid}   
        </select>
 
        <update id="update" parameterType="All" >
            update Buy_Type set Uid=#{Uid},Acredits=#{Acredits}  where Uid=#{Uid}   
        </update>
        <select id="list" resultType="All">
            select * from Buy_Type     
        </select>    
    </mapper>