<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">	
    <mapper namespace="com.mybean.mapper.AdminMapper">
    	
        <insert id="add" parameterType="Admin" >
            insert into admin_info (aId,aName,aSex,aTel,aPhoNum,aAddr,aLimit,aDate) values (#{aId},#{aName},#{aSex},#{aTel},#{aPhoNum},#{aAddr},#{aLimit},#{aDate})   
        </insert>
         
        <delete id="delete" parameterType="_int" >
            delete from admin_info where aId= #{aId}  
        </delete>
         
        <select id="get" parameterType="_int" resultType="Admin">
            select * from admin_info  where aId= #{aId}   
        </select>
 
        <update id="update" parameterType="Admin" >
            update admin_info set aName=#{aName},aSex=#{aSex},aTel=#{aTel},aPhoNum=#{aPhoNum},aAddr=#{aAddr},aLimit=#{aLimit},aDate=#{aDate}  where aId=#{aId}   
        </update>
        <select id="list" resultType="Admin">
            select * from admin_info     
        </select>    
    </mapper>